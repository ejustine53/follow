<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.senzighe.emma.follow.CategoriesActivity"
    android:background="@drawable/backgroundtexture"
    android:padding="0dp">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="60dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="#ff3fa2ff"
        android:paddingLeft="15dp"
        android:paddingTop="13dp"
        android:paddingRight="15dp"
        android:paddingBottom="13dp"
        android:id="@+id/linearLayout5">

        <ImageView
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:id="@+id/imageView18"
            android:src="@drawable/headerlogo"
            android:layout_marginRight="20dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Categories"
            android:layout_marginRight="50dp"
            android:id="@+id/textView4"
            android:textColor="#ffffff"
            android:textSize="@dimen/abc_text_size_headline_material" />

        <ImageView
            android:layout_width="25dp"
            android:layout_height="wrap_content"
            android:id="@+id/imageView20"
            android:src="@drawable/favs"
            android:layout_marginRight="10dp" />

        <ImageView
            android:layout_width="25dp"
            android:layout_height="wrap_content"
            android:id="@+id/imageView19"
            android:src="@drawable/addfav" />
    </LinearLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/linearLayout5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:paddingTop="30dp"
        android:paddingLeft="15dp"
        android:paddingRight="15dp">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="170dp"
            android:layout_height="150dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:background="@drawable/categcontain"
            android:id="@+id/linearLayout6"
            android:layout_marginBottom="15dp">

            <ImageView
                android:layout_width="170dp"
                android:layout_height="120dp"
                android:id="@+id/imageView15"
                android:src="@drawable/spir" />

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="170dp"
            android:layout_height="150dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:background="@drawable/categcontain"
            android:id="@+id/linearLayout7"
            android:layout_marginBottom="15dp"
            android:layout_marginRight="15dp">

            <ImageView
                android:layout_width="170dp"
                android:layout_height="130dp"
                android:id="@+id/imageView16"
                android:src="@drawable/spir"
                android:layout_marginRight="15dp" />

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="170dp"
            android:layout_height="150dp"
            android:background="@drawable/categcontain"
            android:id="@+id/linearLayout8"
            android:layout_alignTop="@+id/linearLayout9"
            android:layout_toRightOf="@+id/linearLayout9"
            android:layout_toEndOf="@+id/linearLayout9">

            <ImageView
                android:layout_width="170dp"
                android:layout_height="120dp"
                android:id="@+id/imageView17"
                android:src="@drawable/orgs" />

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="170dp"
            android:layout_height="150dp"
            android:background="@drawable/categcontain"
            android:id="@+id/linearLayout9"
            android:layout_below="@+id/linearLayout6"
            android:layout_alignLeft="@+id/linearLayout6"
            android:layout_alignStart="@+id/linearLayout6"
            android:layout_marginRight="15dp">

            <ImageView
                android:layout_width="170dp"
                android:layout_height="120dp"
                android:id="@+id/imageView1922"
                android:src="@drawable/edu" />

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="170dp"
            android:layout_height="150dp"
            android:background="@drawable/categcontain"
            android:id="@+id/linearLayout10"
            android:layout_alignTop="@+id/linearLayout6"
            android:layout_alignLeft="@+id/linearLayout8"
            android:layout_alignStart="@+id/linearLayout8">

            <ImageView
                android:layout_width="170dp"
                android:layout_height="120dp"
                android:id="@+id/imageView2066"
                android:src="@drawable/ent" />

        </LinearLayout>
    </RelativeLayout>


  </RelativeLayout>

